import java.util.Scanner;

class Main {
    void find_biggest_in_array(){
    Scanner sc;
    int n;
    sc=new Scanner(System.in);
    System.out.println("Enter number of terms in array");
    n=sc.nextInt();
    int arr[]=new int[n];
    for(int i=0;i<n;i++){
        System.out.println("Enter "+i+"term of array");
        arr[i]=sc.nextInt();
    }
    int smallest=arr[0];
    int largest=arr[0];
    for(int i=0;i<n;i++){
        if(smallest>arr[i]){smallest=arr[i];}
        if(largest<arr[i]){largest=arr[i];}
    }
    System.out.println("Biggest number in arr -> "+largest);
    System.out.println("Smallest number in arr -> "+smallest);
    }
    
    public static void main(String[] args) {
        Main obj=new Main();
        obj.find_biggest_in_array();
    }
}
