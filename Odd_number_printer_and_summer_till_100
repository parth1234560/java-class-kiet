import java.util.Scanner;
class mycompose{
    int a=0;
    void odd_sum_printer_100(){
        for(int i=0;i<=100;i++){
            if(i%2!=0){
                a=a+i;
                System.out.println(i);
            }
        }
        System.out.println("Sum of all odd numbers:" + a);
    }
    public static void main(String[] arg){
        mycompose obj=new mycompose();
        obj.odd_sum_printer_100();
    }
    
}
